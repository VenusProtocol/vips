{
  "name": "@venusprotocol/vips",
  "description": "Venus Protocol VIP simulation",
  "version": "0.0.1",
  "author": "",
  "scripts": {
    "compile": "hardhat compile && hardhat compile --config hardhat.config.zksync.ts",
    "test": "hardhat test",
    "build": "rm -rf dist && tsc --declaration && hardhat compile && hardhat compile --config hardhat.config.zksync.ts && cp -r ./{package.json,yarn.lock,artifacts,contracts,networks} dist/",
    "publish:dist": "yarn build && cd dist && yarn publish --access public",
    "lint": "yarn lint:ts && yarn prettier:check",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --write \"**/*.{js,json,md,ts,yaml,yml,sol}\"",
    "prettier:check": "prettier --check \"**/*.{js,json,md,ts,yaml,yml,sol}\"",
    "docgen": "hardhat docgen",
    "clean": "hardhat clean && hardhat clean --config hardhat.config.zksync.ts",
    "local-test-node:zksyncmainnet": "hardhat node-zksync --fork https://mainnet.era.zksync.io --config hardhat.config.zksync.ts --tag 0.1.0-alpha.31",
    "local-test-node:zksyncsepolia": "hardhat node-zksync --fork https://sepolia.era.zksync.dev --config hardhat.config.zksync.ts --tag 0.1.0-alpha.31",
    "local-anvil-node:zksyncsepolia": "anvil-zksync --gas-price 2000000000 fork --fork-url sepolia-testnet",
    "local-anvil-node:zksyncmainnet": "anvil-zksync --gas-price 3000000000 fork --fork-url mainnet"
  },
  "dependencies": {
    "@morpho-labs/gnosis-tx-builder": "^1.3.1",
    "@openzeppelin/contracts": "^4.8.2",
    "@openzeppelin/contracts-upgradeable": "^4.8.2",
    "@venusprotocol/governance-contracts": "^2.9.0",
    "@venusprotocol/isolated-pools": "^2.5.0",
    "@venusprotocol/oracle": "^1.10.0",
    "@venusprotocol/token-bridge": "^1.1.0",
    "@venusprotocol/venus-protocol": "^7.4.0",
    "ajv": "^8.12.0",
    "decimal.js": "^10.4.3",
    "module-alias": "^2.2.3",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@defi-wonderland/smock": "^2.3.4",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@matterlabs/hardhat-zksync": "^0.2.0",
    "@matterlabs/hardhat-zksync-node": "^1.2.0",
    "@matterlabs/hardhat-zksync-solc": "^1.2.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@safe-global/protocol-kit": "^1.2.0",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/cli-progress": "^3.11.6",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^18.11.9",
    "@types/readline-sync": "^1.4.8",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "chai": "^4.3.7",
    "cli-progress": "^3.12.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.7.2",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.22.15",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.4",
    "sinon": "^15.0.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "_moduleAliases": {
    "src": "src"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "packageManager": "yarn@3.2.1",
  "publishConfig": {
    "access": "public"
  }
}
